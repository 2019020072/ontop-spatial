		<dependency>
			<groupId>org.openrdf.sesame</groupId>
			<artifactId>sesame-sparql-compliance</artifactId>
			<version>2.6.9</version>
			<type>war</type>
		</dependency>
		<dependency>
			<groupId>org.openrdf.sesame</groupId>
