PREFIX : <http://www.owl-ontologies.com/Ontology1207768242.owl#>SELECT DISTINCT * WHERE {      $x a :Address . 		$x :addressID $id. 		$x :inStreet $street. 		$x :inCity $city. 		$x :inCountry $country. 		$x :inState $state. 		$x :hasNumber $number. FILTER ( $country = "Italy"^^<http://www.w3.org/2001/XMLSchema#string> ||  ( $street = "Via Marconi"^^<http://www.w3.org/2001/XMLSchema#string> &&  ( $city = "Bolzano"^^<http://www.w3.org/2001/XMLSchema#string> ||  $street = "Romer Street"^^<http://www.w3.org/2001/XMLSchema#string> ) ) ).}