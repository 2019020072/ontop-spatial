[PrefixDeclaration]
:		http://www.example.org/swrl/1#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
quest:		http://obda.org/quest#

[SourceDeclaration]
sourceUri	person
connectionUrl	jdbc:hsqldb:mem:questjunitdb
username	sa
password    
driverClass	org.hsqldb.jdbc.JDBCDriver

[MappingDeclaration] @collection [[
mappingId	person-map
target		:person/{ID}/ a :Person ; :name {NAME} ; :hasCar {HAS_CAR} ; :age {AGE} .
source		select ID, NAME, HAS_CAR, AGE from PERSON

mappingId	woman-map
target		:person/{ID}/ a :Woman .
source		select ID from PERSON WHERE GENDER='F'

mappingId	man-map
target		:person/{ID}/ a :Man .
source		select ID from PERSON WHERE GENDER='M'

mappingId	spouse-map
target		:person/{ID1}/ :hasSpouse :person/{ID2}/ .
source		select ID1, ID2 from SPOUSE

mappingId	parent-map
target		:person/{CHILD_ID}/ :hasParent :person/{CHILD_ID}/ .
source		select CHILD_ID, PARENT_ID from PARENT
]]

