    <!-- http://www.mgdis.fr/ontologies/2011/01/Operation_noyau.owl#APAEConcept1 -->

    <owl:Class rdf:about="#APAEConcept1">
        <rdfs:label rdf:datatype="&xsd;string"
            >Budget Primitif AP</rdfs:label>
        <rdfs:subClassOf rdf:resource="#BudgetPluriannuel"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Consolidation des AP &#224; l&#39;ann&#233;e N</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.mgdis.fr/ontologies/2011/01/Operation_noyau.owl#APAEConcept2 -->

