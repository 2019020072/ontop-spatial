mappingId	MAPPING-ID2
target		:{ID} a :NUMBER ; :ID2 {ID}^^xsd:integer ; :REFERP {REFERP}^^xsd:integer ; :TYPE_LETTER {TYPE_LETTER} . 
source		SELECT ID,REFERP, TYPE_LETTER FROM NUMBER WHERE REFERP='2'

mappingId	MAPPING-ID3
target		:{ID} a :NUMBER ; :ID3 {ID}^^xsd:integer ; :REFERQ {REFERQ}^^xsd:integer ; :TYPE_LETTER {TYPE_LETTER} . 
source		SELECT ID,REFERQ, TYPE_LETTER FROM NUMBER WHERE  REFERQ='3' OR REFERQ='4'
]]

