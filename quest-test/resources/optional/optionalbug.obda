[MappingDeclaration] @collection [[
mappingId	MAPPING-ID1
target		:{ID} a :NUMBER ; :ID1 {ID}^^xsd:integer ; :REFERP {REFERP}^^xsd:integer ; :TYPE_LETTER {TYPE_LETTER} . 
