[PrefixDeclaration]
:		http://it.unibz.krdb/obda/test/simple#
number: http://it.unibz.krdb/obda/test/simple/NUMBER#

[SourceDeclaration]
sourceUri	optional
connectionUrl	jdbc:h2:mem:questjunitdb
username	fish
password	fish
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[
mappingId	MAPPING-ID1
target		:ID1{ID} a :NUMBER ; :REFERP {REFERP}^^xsd:integer ; :TYPE_LETTER {TYPE_LETTER} . 
source		SELECT ID,REFERP, TYPE_LETTER FROM NUMBER WHERE REFERP='1'

mappingId	MAPPING-ID2
target		:ID2{ID} a :NUMBER ; :REFERP {REFERP}^^xsd:integer ; :TYPE_LETTER {TYPE_LETTER} . 
source		SELECT ID,REFERP, TYPE_LETTER FROM NUMBER WHERE REFERP='2'

mappingId	MAPPING-ID3
target		:ID3{ID} a :NUMBER ; :REFERQ {REFERQ}^^xsd:integer ; :TYPE_LETTER {TYPE_LETTER} . 
source		SELECT ID,REFERQ, TYPE_LETTER FROM NUMBER WHERE  REFERQ='3' OR REFERQ='4'
]]

