[PrefixDeclaration]
:		http://obda.inf.unibz.it/sparql/test-hierarchy.owl#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
test:		http://obda.inf.unibz.it/sparql/test-hierarchy.owl#
owl2xml:		http://www.w3.org/2006/12/owl2-xml#

[SourceDeclaration]
sourceUri	h2
connectionUrl	jdbc:h2:mem:questjunitdb
username	sa
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[

mappingId	Person
target		:{p_id} a :Person . 
source		select p_id, p_name from people

mappingId	Man
target		:{p_id} a :Man . 
source		select p_id from people where sex='M'

mappingId	Woman
target		:{p_id} a :Woman . 
source		select p_id from people where sex='F'

mappingId	isAffilatedTo
target		:{f_id} :isRelatedTo :{relative} . 
source		select f_id, relative from family

mappingId	isSibling
target		:{f_id}  :isSibling :{relative} . 
source		select id, relative from family where relation = 'S'

mappingId	isRelative
target		:{f_id} :isSibling :{relative} . 
source		select id, relative from family where relation = 'R'

mappingId	isAncestor
target		:{f_id} :isAncestor :{relative} . 
source		select f_id, relative from family where relation = 'G' 

mappingId	isParent
target		:{f_id} :isParent :{relative} . 
source		select f_id, relative from family where relation = 'P'

]]

