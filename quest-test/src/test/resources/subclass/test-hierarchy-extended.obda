[PrefixDeclaration]
:		http://obda.inf.unibz.it/sparql/test-hierarchy.owl#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
xsd:		http://www.w3.org/2001/XMLSchema#
rdfs:		http://www.w3.org/2000/01/rdf-schema#
test:		http://obda.inf.unibz.it/sparql/test-hierarchy.owl#
owl2xml:		http://www.w3.org/2006/12/owl2-xml#

[SourceDeclaration]
sourceUri	h2
connectionUrl	jdbc:h2:mem:questjunitdb
username	sa
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[

mappingId	Person
target		:person-{p_id} a :Person . 
source		select p_id from people

mappingId	Male
target		:person-{p_id} a :Male . 
source		select p_id from people where sex='M'

mappingId	Man
target		:person-{p_id} a :Man . 
source		select p_id from people where ageInterval='M'

mappingId	Woman
target		:person-{p_id} a :Woman . 
source		select p_id from people where ageInterval='W'

mappingId	Female
target		:person-{p_id} a :Female . 
source		select p_id from people where sex='F'

mappingId	isAffilatedTo
target		:person-{f_id} :isRelatedTo :person-{relative} . 
source		select f_id, relative from family

mappingId	isSibling
target		:person-{f_id}  :isSibling :person-{relative} . 
source		select id, relative from family where relation = 'S'

mappingId	isRelative
target		:person-{f_id} :isSibling :person-{relative} . 
source		select id, relative from family where relation = 'R'

mappingId	isAncestor
target		:person-{f_id} :isAncestor :person-{relative} . 
source		select f_id, relative from family where relation = 'G' 

mappingId	isParent
target		:person-{f_id} :isParent :person-{relative} . 
source		select f_id, relative from family where relation = 'P'


]]

