[PrefixDeclaration]
:		http://ontop.inf.unibz.it/test/datatypes#
xsd:	http://www.w3.org/2001/XMLSchema#

[SourceDeclaration]
sourceUri	h2
connectionUrl	jdbc:h2:mem:datatypes
username	sa	
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[
mappingId	Character Types
target		:character-{id} a :Row ; :hasVarChar {type_varchar} ; :hasCharacter {type_character} ; :hasText {type_text} ; :hasChar {type_char}; :hasLongText {type_longtext} ; :hasMediumText {type_longtext} ; :hasTinyText {type_tinytext} .   .
source		select id, type_varchar, type_character, type_text, type_char, type_longtext, type_mediumtext,  from "Characters"

mappingId	Date-Time Types
target		:datetime-{id} a :Row ; :hasTime {type_time} ; :hasTimeString {type_time}^^xsd:string ; :hasDate {type_date}^^xsd:date ; :hasDateString {type_date}^^xsd:string ; :hasYear {type_year}^^xsd:gYear ; :hasTimestamp {type_timestamp} ; :hasTimestampString {type_timestamp}^^xsd:string .
source		select id, type_timestamp, type_year, type_date, type_time from "DateTimes"

mappingId	Boolean Types
target		:boolean-{id} a :Row ; :hasBoolean {type_boolean} .
source		select id, type_boolean from "Booleans"


]]

