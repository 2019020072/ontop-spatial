quest:		http://obda.org/quest#

[SourceDeclaration]
sourceUri	http://www.example.org/db/dummy2/
connectionUrl	jdbc:postgresql://www.example.org/dummy2
username	dummy2
password	dummy2
driverClass	org.postgresl.Driver

[MappingDeclaration] @collection {
mappingId	M4
targetQuery	<"&:;P{$id}"> a :Student; :firstName $nome; :lastName $cognome; :age $eta^^xsd:integer . 
sourceQuery	select id, nome, cognome, eta from studenti

mappingId	M5
targetQuery	<"&:;C{$id}"> a :Course; :title $titolo; :lecturer $professore; :description $decrizione@it . 
sourceQuery	select id, titolo, professore, descrizione from corso

mappingId	M6
targetQuery	<"&:;P{$sid}"> :hasEnrollment <"&:;C{$cid}"> . 
sourceQuery	select sid, cid from registrare
}

[SourceDeclaration]
sourceUri	http://www.example.org/db/dummy/
connectionUrl	jdbc:postgresql://www.example.org/dummy
username	dummy
password	dummy
driverClass	org.postgresl.Driver

[MappingDeclaration] @collection {
mappingId	M1
targetQuery	<"&:;P{$id}"> a :Student; :firstName $fname; :lastName $lname; :age $age^^xsd:integer . 
sourceQuery	select id, fname, lname, age from student

mappingId	M2
targetQuery	<"&:;C{$id}"> a :Course; :title $title; :lecturer $lecturer; :description $description@en-US . 
sourceQuery	select id, title, lecturer, description from course

mappingId	M3
targetQuery	<"&:;P{$sid}"> :hasEnrollment <"&:;C{$cid}"> . 
sourceQuery	select sid, cid from enrollment
}

